    "    with open(caminho, 'r') as arquivo:\n",
    "        return arquivo.read()\n",
    "\n",
    "arquivo = ler(\"zi.txt\")\n",
    "\n",
    "nomes = [arquivo.strip() for arquivo in arquivo.split(\",\")]\n",
    "\n",
    "for nome in nomes:\n",
    "    print(nome)\n"
